{
    "name": "org-emacs-export",
    "displayName": "Org Emacs Export",
    "description": "Org Emacs Export functionalities for VSCode",
    "version": "0.1.0",
    "publisher": "vscode-org-mode",
    "license": "SEE LICENSE IN LICENSE.txt",
    "repository": {
        "type": "git",
        "url": "https://github.com/vscode-org-mode/org-emacs-export"
    },
    "icon": "images/logo.png",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:org"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "org.asciiExportToAscii",
                "title": "Org: export to ascii"
            },
            {
                "command": "org.beamerExportToLatex",
                "title": "Org: export to beamer latex"
            },
            {
                "command": "org.beamerExportToPdf",
                "title": "Org: export to beamer pdf"
            },
            {
                "command": "org.htmlExportToHtml",
                "title": "Org: export to html"
            },
            {
                "command": "org.icalendarExportToIcs",
                "title": "Org: export to ics"
            },
            {
                "command": "org.latexExportToLatex",
                "title": "Org: export to latex"
            },
            {
                "command": "org.latexExportToPdf",
                "title": "Org: export to pdf"
            },
            {
                "command": "org.mdExportToMarkdown",
                "title": "Org: export to markdown"
            },
            {
                "command": "org.odtExportToOdt",
                "title": "Org: export to odt"
            }
        ],
        "languages": [
            {
                "id": "org",
                "aliases": [
                    "Org",
                    "Org Markup",
                    "Org Mode"
                ],
                "extensions": [
                    ".org"
                ],
                "configuration": "./org.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "org",
                "scopeName": "source.org",
                "path": "./syntaxes/org.tmLanguage.json"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Org Emacs Export configuration",
            "properties": {
                "org.export.showOutputChannel": {
                    "type": "boolean",
                    "default": true,
                    "description": "Display or not the Org output channel when exporting."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "date-fns": "^1.28.3"
    }
}
